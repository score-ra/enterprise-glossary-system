# Environment
ENVIRONMENT=development

# Fuseki Configuration
FUSEKI_PORT=3030
FUSEKI_ADMIN_PASSWORD=your-admin-password-here

# Varnish Cache
CACHE_PORT=9031

# SKOSMOS Configuration (direct access, bypasses gateway)
SKOSMOS_PORT=9090

# Nginx Gateway (public entry point with RBAC)
GATEWAY_PORT=8080

# RBAC Credentials (used by scripts/setup-auth.sh)
EGMS_ADMIN_USER=admin
EGMS_ADMIN_PASS=your-admin-password-here
EGMS_EDITOR_USER=editor
EGMS_EDITOR_PASS=your-editor-password-here

# Data Loading (used by scripts)
FUSEKI_URL=http://localhost:3030
FUSEKI_USER=admin
FUSEKI_PASS=your-admin-password-here
GRAPH_URI=http://glossary.example.org/
DATASET=skosmos
